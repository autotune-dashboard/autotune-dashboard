{
  "name": "autotune-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "serve build",
    "start:dev": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "deploy": "now switch autotune-dashboard && now --public && now alias -A now.json",
    "ci:now:deploy": "now -t $NOW_TOKEN -T $NOW_TEAM -p",
    "ci:now:alias": "now alias -A now.json -t $NOW_TOKEN -T $NOW_TEAM",
    "ci:now:clean": "now rm $NOW_PROJECT -t $NOW_TOKEN -T $NOW_TEAM --safe -y"
  },
  "dependencies": {
    "@material-ui/core": "^1.4.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts-ts": "2.16.0",
    "serve": "^9.4.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/node": "^10.5.4",
    "@types/react": "^16.4.7",
    "@types/react-dom": "^16.0.6",
    "typescript": "^2.9.2"
  }
}
